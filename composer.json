        "wdigital",
        "klicktipp-for-laravel"
    ],
    "homepage": "https://github.com/wdigital/klicktipp-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Florian WÃ¼rtenberger",
