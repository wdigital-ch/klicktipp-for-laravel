    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0",
        "facade/ignition"    : "^1.4|^2.3.6"
    },
    "autoload": {
        "psr-4": {
            "WDigital\\KlickTippForLaravel\\": "src/"
        }
