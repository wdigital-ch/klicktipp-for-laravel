    },
    "extra": {
        "laravel": {
            "providers": [
                "WDigital\\KlickTippForLaravel\\KlickTippForLaravelServiceProvider"
            ],
            "aliases": {
                "KlickTippForLaravel": "WDigital\\KlickTippForLaravel\\KlickTippForLaravelFacade"
            }
        }
